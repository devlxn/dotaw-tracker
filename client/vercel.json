{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "img-src 'self' https://cdn.dota2.com https://steamcommunity.com https://avatars.steamstatic.com data:;"
        }
      ]
    }
  ]
}
